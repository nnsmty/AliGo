{
  "manifest_version": 2,

  "name": "Ali Go",
  "description": "Xi'an Technology ",
  "version": "1.0.1",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
	
  },
  
  "content_scripts": [
    {
		"run_at": "document_idle",
  // "matches": ["<all_urls>","http://*/*","https://*/*"],
   "matches": ["https://www.aliexpress.com/*/*","http://www.aliexpress.com/*/*"],
	  "js": ["inject.js","jquery.min.js","search_product.js","single_product.js","popup.js"],
      "css": ["search_product.css"]   
	  
	  
    }
					],
  
   "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab", "tabs","contextMenus"],
	"web_accessible_resources": [
			"jquery.min.js",
			"search_product.js",
			"single_product.js",
			"popup.js",			
			"icon.png",
			"icon_68.png"
			
			
			]
			
	
			


 
  
}


 